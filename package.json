    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "graphql": "^16.12.0",
    "graphql-scalars": "^1.25.0",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^5.8.2",
    "keyv": "^5.5.4",
    "prisma": "^6.19.0",
    "prisma-graphql-type-decimal": "^3.0.1",
    "reflect-metadata": "^0.2.2",
